;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Rule",
"language": "GDScript",
"path": "res://generator/rules/Rule.gd"
}, {
"base": "Node",
"class": "TYPE_EDGE",
"language": "GDScript",
"path": "res://generator/graph/edge/TYPE_EDGE.gd"
}, {
"base": "Node",
"class": "TYPE_VERTEX",
"language": "GDScript",
"path": "res://generator/graph/vertex/TYPE_VERTEX.gd"
} ]
_global_script_class_icons={
"Rule": "",
"TYPE_EDGE": "",
"TYPE_VERTEX": ""
}

[application]

config/name="level generator graph"
run/low_processor_mode=true
config/icon="res://icon.png"

[global]

os=false
ide=false

[physics]

common/enable_pause_aware_picking=true
2d/default_gravity=0

[rendering]

environment/default_environment="res://default_env.tres"
